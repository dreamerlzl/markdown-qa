{
  "productName": "Markdown Q&A",
  "version": "0.1.0",
  "identifier": "app.markdown-qa.gui",
  "build": {
    "frontendDist": ".."
  },
  "app": {
    "security": {
      "csp": "default-src 'self' 'unsafe-inline' 'unsafe-eval' http: https: tauri:; img-src 'self' data: http: https: tauri:"
